Program Pzim ;
Var

 Primeiro, Segundo, soma, subtracao, multiplicacao, divisao, total: real ; 
Begin
  Writeln (' Primeiro Numero ? ') ;
  readln ( Primeiro ) ;
  
  Writeln (' Segundo Numero ? ');
  readln ( Segundo ) ;
  
  Write (' Soma é ') ;
  Writeln (  Primeiro + Segundo ) ;
  Write (' Subtração é ') ;
	Writeln ( Primeiro - Segundo ) ;
  Write (' Multiplicação é ') ;
	Writeln ( Primeiro * Segundo ) ;
  Write (' Divisão é ') ;
	Writeln ( Primeiro / Segundo ) ;
End.